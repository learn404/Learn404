{
    "compilerOptions": {
      "target": "es6",
      "lib": ["dom", "es6"],
      "allowJs": true,
      "jsx": "react",
      "moduleResolution": "node",
      "allowSyntheticDefaultImports": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "noImplicitAny": true,
      "module": "esnext",
      "resolveJsonModule": true,
      "isolatedModules": true
    },
    "include": ["src"]
  }
  